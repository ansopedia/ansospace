{
  "name": "@ansospace/types",
  "version": "0.0.1",
  "description": "Shared TypeScript type definitions for the AnsoSpace platform",
  "keywords": [
    "typescript",
    "types",
    "ansospace"
  ],
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "publish-packages": "turbo run build lint test && changeset version && changeset publish"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts"
    },
    "./user": {
      "types": "./src/user.ts"
    },
    "./course": {
      "types": "./src/course.ts"
    }
  },
  "devDependencies": {
    "@ansospace/typescript-config": "workspace:*",
    "typescript": "5.9.2"
  }
}
