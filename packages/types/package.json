{
  "name": "@ansospace/types",
  "version": "0.3.8",
  "description": "Shared TypeScript type definitions for the AnsoSpace platform",
  "keywords": [
    "typescript",
    "types",
    "ansospace"
  ],
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsup src/index.ts --format esm --dts",
    "publish-packages": "turbo run build lint && changeset version && changeset publish",
    "prepublishOnly": "pnpm build"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "dependencies": {
    "bson": "^6.10.4",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@ansospace/typescript-config": "workspace:*",
    "tsup": "^8.5.0",
    "typescript": "5.9.3"
  },
  "files": [
    "dist",
    "README.md",
    "package.json",
    "CHANGELOG.md"
  ]
}
