{
  "name": "@ansospace/types",
  "version": "0.2.0",
  "description": "Shared TypeScript type definitions for the AnsoSpace platform",
  "keywords": [
    "typescript",
    "types",
    "ansospace"
  ],
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "publish-packages": "turbo run build lint test && changeset version && changeset publish"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "dependencies": {
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@ansospace/typescript-config": "workspace:*",
    "typescript": "5.9.2"
  }
}
